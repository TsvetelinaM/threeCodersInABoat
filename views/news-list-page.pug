if news[0].fields    
	each val in news
		.col-sm-6.col-xs-12.col-md-3.news-middle-thumbnail
			a(href='')
				.hover-cover
				if val.fields.thumbnail
					img(src= val.fields.thumbnail, alt='')
				else
					img(src='./../static/imgs/defaultArticle.png', alt='')
				h2= val.fields.headline
				p= val.fields.trailText